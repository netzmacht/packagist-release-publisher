{
    "name": "netzmacht/packagist-release-publisher",
    "description": "Ths package provides a CLI tool to create release notes of packagist releases.",
    "type": "project",
    "require": {
        "php": "~7.1",
        "ext-posix": "*",
        "ext-json": "*",
        "guzzlehttp/psr7": "^1.4",
        "guzzlehttp/guzzle": "^6.3",
        "netzmacht/tapatalk-client-api": "^0.2.0",
        "symfony/console": "^4.1",
        "symfony/filesystem": "^4.1",
        "zendframework/zend-feed": "^2.10",
        "composer/semver": "^1.4",
        "psr/container": "^1.0",
        "zendframework/zend-servicemanager": "^3.3",
        "webmozart/path-util": "^2.3"
    },
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "David Molineus",
            "email": "david.molineus@netzmacht.de"
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "require-dev": {
        "phpcq/all-tasks": "^1.2"
    },
    "bin": ["bin/packagist-release-publisher"]
}
